{
  "bindings": [
    {
      "type": "timerTrigger",
      "name": "myTimer",
      "schedule": "0 0 * * * *",
      "direction": "in"
    },
    {
      "type": "sendGrid",
      "name": "$return",
      "direction": "out",
      "apiKey": "SendGridAPIKey",
      "from": "Data Sync Failure Monitor <functions@azure.com>"
    },
    {
      "type": "blob",
      "name": "myInputBlob",
      "path": "DataSyncFailureMonitor/lastCheck",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "type": "blob",
      "name": "myOutputBlob",
      "path": "DataSyncFailureMonitor/lastCheck",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ],
  "disabled": false
}